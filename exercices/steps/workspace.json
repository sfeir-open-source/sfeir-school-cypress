    "nest-server": {
      "root": "apps/nest-server",
      "sourceRoot": "apps/nest-server/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nest-server",
            "main": "apps/nest-server/src/main.ts",
            "tsConfig": "apps/nest-server/tsconfig.app.json",
            "assets": ["apps/nest-server/src/assets"],
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest-server/src/environments/environment.ts",
                  "with": "apps/nest-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "nest-server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/nest-server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nest-server"],
          "options": {
            "jestConfig": "apps/nest-server/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "react-solution": {
      "root": "apps/react/solution",
      "sourceRoot": "apps/react/solution/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/react/solution",
            "index": "apps/react/solution/src/index.html",
            "baseHref": "/",
            "main": "apps/react/solution/src/main.tsx",
            "polyfills": "apps/react/solution/src/polyfills.ts",
            "tsConfig": "apps/react/solution/tsconfig.app.json",
            "assets": ["apps/react/solution/src/favicon.ico", "apps/react/solution/src/assets"],
            "styles": ["apps/react/solution/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack",
            "postcssConfig": "apps/react/solution/postcss.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react/solution/src/environments/environment.ts",
                  "with": "apps/react/solution/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/webpack:dev-server",
          "options": {
            "buildTarget": "react-solution:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "react-solution:build:production",
              "hmr": false
            },
            "development": {
              "buildTarget": "react-solution:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/react/solution/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react/solution"],
          "options": {
            "jestConfig": "apps/react/solution/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
